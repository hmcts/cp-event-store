<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.3.xsd">

    <changeSet id="stream_buffer-017" author="TechPod"
               logicalFilePath="liquibase/event-buffer-changesets/017-add-index-on-error-hash-in-stream-status.changelog.xml">


        <createIndex tableName="stream_error"  indexName="stream_error.hash.idx">
            <column name="hash"/>
        </createIndex>

        <rollback>
            <dropIndex indexName="stream_error.hash.idx" tableName="stream_error"/>
        </rollback>

    </changeSet>

</databaseChangeLog>
